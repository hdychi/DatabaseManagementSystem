<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.databasemanagementsystem">
     <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>  
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/> 
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="学生信息管理系统"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UI.InsertChooseAcrivity"></activity>
        <activity android:name=".UI.InsertCourseActivity"></activity>
        <activity android:name=".UI.InsertStudentActivity"></activity>
        <activity android:name=".UI.UpdateStudentActivity"></activity>
        <activity android:name=".UI.UpdateCourseActivity"></activity>
        <activity android:name=".UI.UpdateChooseFromStudentActivity"></activity>
        <activity android:name=".UI.UpdateChooseFromCourseActivity"></activity>
    </application>

</manifest>